{
  "name": "your-app",
  "version": "1.0.0",
  "scripts": {
    "start:server": "cd server && node index.js",
    "start:client": "cd client && npm install && npm run build",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client dev": "cd client && npm run dev",
    "start dev": "concurrently \"npm run start:server\" \"npm run start:client dev\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/material": "^5.14.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-slider": "^2.0.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": "18.17.0"
  }
}
